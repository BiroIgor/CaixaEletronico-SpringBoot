# Configuração NetBeans para projeto Maven Spring Boot
annotation.processing.enabled=true
annotation.processing.enabled.in.editor=false
annotation.processing.processors.list=
annotation.processing.run.all.processors=true
application.title=Caixa Eletrônico Spring Boot
application.vendor=Igor
build.classes.dir=${build.dir}/classes
build.classes.excludes=**/*.java,**/*.form
compile.on.save=true
do.depend=false
do.jar=true

# Configurações Maven
maven.archetypeGroupId=org.springframework.boot
maven.archetypeArtifactId=spring-boot-starter-parent
maven.archetypeVersion=3.1.0

# Configurações Java
java.source.based=true
javac.source=17
javac.target=17
javac.compilerargs=
javac.deprecation=false
javac.external.vm=true
javac.processorpath=${javac.classpath}
javac.test.classpath=${javac.classpath}:${build.classes.dir}

# Documentação
javadoc.encoding=${source.encoding}
javadoc.html5=false
javadoc.noindex=false
javadoc.nonavbar=false
javadoc.notree=false
javadoc.private=false
javadoc.splitindex=true
javadoc.use=true
javadoc.version=false
javadoc.windowtitle=

# Main class para execução
main.class=br.com.Igor.caixaeletronico.CaixaEletronicoApplication

# Configurações de execução
run.classpath=${javac.classpath}:${build.classes.dir}
run.jvmargs=-Dspring.profiles.active=dev
run.modulepath=${javac.modulepath}
run.test.classpath=${javac.test.classpath}:${build.test.classes.dir}
run.test.modulepath=${javac.test.modulepath}
source.encoding=UTF-8
